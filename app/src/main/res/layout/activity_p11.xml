<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.p11">

    <ImageButton
        android:id="@+id/Initialbutton"
        android:layout_width="500dp"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"
        android:src="@drawable/d8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/i"
        android:layout_width="291dp"
        android:layout_height="81dp"
        android:gravity="center"
        android:text="Pet Rental"
        android:fontFamily="@font/myfont1"
        android:textColor="@color/white"
        android:textSize="50sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/Initialbutton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/Initialbutton"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.74" />

    <TextView
        android:id="@+id/i1"
        android:layout_width="291dp"
        android:layout_height="81dp"
        android:gravity="center"
        android:text="Rent Happiness at your Doorsteps"
        android:textColor="@color/white"
        android:textSize="18sp"
        android:textStyle="normal"
        android:fontFamily="@font/myfont1"
        app:layout_constraintBottom_toBottomOf="@+id/Initialbutton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/Initialbutton"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.9" />

</androidx.constraintlayout.widget.ConstraintLayout>